{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "agusb"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/core-js/client/shim.min.js",
        "../node_modules/zone.js/dist/zone.js",
        "../node_modules/reflect-metadata/Reflect.js",
        "assets/js/jquery-2.1.4.min.js",
        "assets/plugins/bootstrap/js/bootstrap.min.js",
        "assets/plugins/smoothscroll/smoothscroll.js",
        "assets/plugins/easing/jquery.easing.1.3.js",
        "assets/plugins/wow/wow.min.js",
        "assets/plugins/counter-up/waypoints.min.js",
        "assets/plugins/counter-up/jquery.counterup.min.js",
        "assets/plugins/imagesloaded/imagesloaded.pkgd.min.js",
        "assets/plugins/isotope/isotope.pkgd.min.js",
        "assets/plugins/magnific-popup/js/jquery.magnific-popup.min.js",
        "assets/plugins/youtube-player/js/jquery.mb.YTPlayer.js",
        "assets/plugins/swiper/js/swiper.min.js",
        "assets/plugins/parallax-scroll-master/jquery.parallax-scroll.js",
        "assets/plugins/validation/jquery.validate.min.js",
        "assets/js/email.js",
        "assets/js/main.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
